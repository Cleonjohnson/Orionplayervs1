{
  "name": "orionplayer2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:apk": "npx eas-cli build --platform android --profile preview --non-interactive",
    "build:apk:prod": "npx eas-cli build --platform android --profile production --non-interactive",
    "build:apk:setup": "npx eas-cli build --platform android --profile preview",
    "install:emulator": "npx eas-cli build:run -p android",
    "fix:assets": "node scripts/fix-asset-pngs.js",
    "generate:icon": "node scripts/generate-app-icon.js",
    "update:preview": "npx eas-cli update --branch preview --message \"OTA update\" --non-interactive",
    "update:production": "npx eas-cli update --branch production --message \"OTA update\" --non-interactive"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/stack": "^7.6.16",
    "@sentry/react-native": "~7.2.0",
    "axios": "^1.13.5",
    "expo": "~54.0.33",
    "expo-av": "~16.0.8",
    "expo-brightness": "~14.0.3",
    "expo-build-properties": "~1.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-keep-awake": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "~0.32.16",
    "expo-screen-orientation": "~9.0.8",
    "expo-secure-store": "~15.0.8",
    "expo-sensors": "~15.0.8",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "expo-video": "~3.0.16",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "jimp": "^0.22.12"
  },
  "private": true
}
